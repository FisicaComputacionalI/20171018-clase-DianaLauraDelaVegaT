//Programa saca el factorial de un numero
//Autor: Diana Laura De la Vega Trinidad.
#include <iostream>
using namespace std;

int main(){
    
    int N=0;
    cout<<"dame un numero entero"<<endl;
    cin>>N;
    
    int M=1;
    int F=1;
    
    while (M!=N){
        M=M+1;
        F=F*M;
    }
    
    cout<<"valor final"<< F<<endl;
    return 0;
}
